{
    "name": "imagina/icurrency-module",
    "description": "This module manages currencies and conversions",
    "type": "asgard-module",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0",
        "imagina/core-module": "4.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Icurrency\\": ".",
            "Modules\\": "Modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Imagina Soluciones Web",
            "email": "soporte@imaginacolombia.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Icurrency\\Providers\\IcurrencyServiceProvider"
            ],
            "aliases": {

            }
        },
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Icurrency\\": ""
        }
    }
}
